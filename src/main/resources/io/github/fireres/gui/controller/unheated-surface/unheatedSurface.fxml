                        fx:id="component"
                        fx:controller="io.github.fireres.gui.controller.unheated.surface.UnheatedSurface">

    <VBox focusTraversable="true">
        <fx:include fx:id="firstGroup"
                    source="groups/first/firstGroup.fxml"/>

        <fx:include fx:id="secondGroup"
                    source="groups/second/secondGroup.fxml"/>

        <fx:include fx:id="thirdGroup"
                    source="groups/third/thirdGroup.fxml"/>
    </VBox>
</ScrollPaneWithoutFocus>





