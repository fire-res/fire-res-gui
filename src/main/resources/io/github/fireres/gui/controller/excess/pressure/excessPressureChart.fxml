<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/10.0.2-internal"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:id="component"
           fx:controller="io.github.fireres.gui.controller.excess.pressure.ExcessPressureChart">
    <LineChart fx:id="chart" focusTraversable="true" legendSide="RIGHT" maxHeight="1.7976931348623157E308"
               legendVisible="false"
               maxWidth="1.7976931348623157E308" prefWidth="5000.0">
        <xAxis>
            <NumberAxis focusTraversable="true" label="Время, мин." side="BOTTOM"/>
        </xAxis>

        <yAxis>
            <NumberAxis side="LEFT"/>
        </yAxis>
    </LineChart>
</StackPane>
